<Skin xmlns="http://ns.adobe.com/mxml/2009" 
	  minWidth="21" minHeight="21"
	  alpha.disabled="0.5">

	<!-- states -->
	<states>
		<State name="up" />
		<State name="over" />
		<State name="down" />
		<State name="disabled" />
	</states>
	
	<!-- layer 1: fill -->
	<Rect left="0" right="0" top="1" bottom="1" excludeFrom="up,disabled" >
		<fill>
			<SolidColor color.over="#2989FF" color.down="#2989FF" alpha.over=".15" alpha.down=".4" />
		</fill>
	</Rect>
	
	<!-- layer 2: fill highlight -->
	<Rect left="0" right="0" top="1" height="9" excludeFrom="up" >
		<fill>
			<SolidColor color="0xFFFFFF" alpha="0.33" />
		</fill>
	</Rect>
	
</Skin>