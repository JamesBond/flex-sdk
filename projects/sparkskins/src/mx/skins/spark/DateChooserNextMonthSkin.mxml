<!--- The Spark skin class for the next month arrow of a Halo DateChooser component. -->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" minWidth="21" minHeight="9" >

	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<s:BitmapImage source="@Embed('assets/DateChooserArrowRight.png')" includeIn="up" right="0" verticalCenter="0" />
	
	<s:BitmapImage source="@Embed('assets/DateChooserArrowRightOver.png')" includeIn="over" right="0" verticalCenter="0" />
	
	<s:BitmapImage source="@Embed('assets/DateChooserArrowRightDown.png')" includeIn="down" right="0" verticalCenter="0" />
	
</s:Skin>