<SparkSkin xmlns="http://ns.adobe.com/mxml/2009" 
	  minWidth="21" minHeight="21" 
	  alpha.disabled="0.5">

	<!-- states -->
	<states>
		<State name="up" />
		<State name="over" />
		<State name="down" />
		<State name="disabled" />
		<State name="selectedUp" />
		<State name="selectedOver" />
		<State name="selectedDown" />
		<State name="selectedDisabled" />
	</states>
	
	<!-- layer 3: fill -->
	<Rect left="1" right="1" top="1" bottom="1">
		<fill>
			<LinearGradient rotation="90">
				<GradientEntry color="0xFFFFFF" 
							   color.over="0xBBBDBD" 
							   color.down="0xAAAAAA" 
							   alpha="0.85" />
				<GradientEntry color="0xD8D8D8" 
							   color.over="0x9FA0A1" 
							   color.down="0x929496" 
							   alpha="0.85" />
			</LinearGradient>
		</fill>
	</Rect>
	
	<!-- layer 4: fill lowlight -->
	<Rect left="1" right="1" bottom="1" height="9">
		<fill>
			<LinearGradient rotation="90">
				<GradientEntry color="0x000000" alpha="0.0099" />
				<GradientEntry color="0x000000" alpha="0.0627" />
			</LinearGradient>
		</fill>
	</Rect>
	
	<!-- layer 5: fill highlight -->
	<Rect left="1" right="1" top="1" height="9">
		<fill>
			<SolidColor color="0xFFFFFF" 
					    alpha="0.33" 
					    alpha.over="0.22" 
					    alpha.down="0.12" />
		</fill>
	</Rect>
	
	<!-- layer 6: highlight stroke (all states except down) -->
	<Rect left="1" right="1" top="1" bottom="1" excludeFrom="down">
		<stroke>
			<LinearGradientStroke rotation="90">
				<GradientEntry color="0xFFFFFF" alpha.over="0.22" />
				<GradientEntry color="0xD8D8D8" alpha.over="0.22" />
			</LinearGradientStroke>
		</stroke>
	</Rect>
	
	<!-- layer 6: highlight stroke (down state only) -->
	<Rect left="1" top="1" bottom="1" width="1" includeIn="down">
		<fill>
			<SolidColor color="0x000000" alpha="0.07" />
		</fill>
	</Rect>
	<Rect right="1" top="1" bottom="1" width="1" includeIn="down">
		<fill>
			<SolidColor color="0x000000" alpha="0.07" />
		</fill>
	</Rect>
	<Rect left="1" top="1" right="1" height="1" includeIn="down">
		<fill>
			<SolidColor color="0x000000" alpha="0.25" />
		</fill>
	</Rect>
	<Rect left="1" top="2" right="1" height="1" includeIn="down">
		<fill>
			<SolidColor color="0x000000" alpha="0.09" />
		</fill>
	</Rect>
	
	<!-- layer 2: border -->
	<Rect left="0" right="0" top="0" bottom="0" width="69" height="20">
		<stroke>
			<SolidColorStroke color="0x696969" 
					    alpha="1" 
					    alpha.over="1" 
					    alpha.down="1" />
		</stroke>
	</Rect>
	
</SparkSkin>