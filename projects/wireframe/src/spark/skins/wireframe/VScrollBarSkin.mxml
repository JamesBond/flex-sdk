<Skin xmlns="http://ns.adobe.com/mxml/2009"
      minWidth="15" minHeight="85"
	  alpha.disabled="0.5">

    <Metadata>
    	[HostComponent("mx.components.FxVScrollBar")]
    </Metadata> 
    
    <states>
    	<State name="normal" />
    	<State name="disabled" />
    </states>

    <FxButton id="track" top="17" bottom="17"
            skinClass="wireframe.FxVScrollBarTrackSkin" />
    <FxButton id="decrementButton" top="0" 
            skinClass="wireframe.FxScrollBarUpButtonSkin" />
    <FxButton id="incrementButton" bottom="0"
            skinClass="wireframe.FxScrollBarDownButtonSkin" />
    <FxButton id="thumb" skinClass="wireframe.FxVScrollBarThumbSkin" />

</Skin>
