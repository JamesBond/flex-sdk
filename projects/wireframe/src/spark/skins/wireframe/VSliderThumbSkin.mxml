<Skin xmlns="http://ns.adobe.com/mxml/2009">

    <Metadata>
    	[HostComponent("mx.components.FxButton")]
    </Metadata> 
    
    <states>
        <State name="up" />
        <State name="over" stateGroups="overAndDown" />
        <State name="down" stateGroups="overAndDown" />
        <State name="disabled" />
    </states>

    <Rect left="0" right="0" top="0" bottom="0" radiusX="7" radiusY="7">
        <stroke>
            <SolidColorStroke color="0x3B5B92" />
        </stroke>
        <fill>
            <SolidColor color="0xFFFFFF" color.overAndDown="0xEEF3FF" />
        </fill>
    </Rect>

</Skin>
