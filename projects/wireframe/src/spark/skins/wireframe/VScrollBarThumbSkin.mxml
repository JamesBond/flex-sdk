<Skin xmlns="http://ns.adobe.com/mxml/2009">

    <Metadata>
    	[HostComponent("mx.components.FxButton")]
    </Metadata> 
    
    <states>
        <State name="up" />
        <State name="over"/>
        <State name="down" />
        <State name="disabled" />
    </states>


    <!-- background -->
    <Rect left="0" top="0" right="0" bottom="0" minWidth="14" minHeight="14">
        <stroke>
            <SolidColorStroke color="0x3D5D92" />
        </stroke>
        <fill>
            <SolidColor color="0xFFFFFF" />
        </fill>
    </Rect>

    <!-- Gripper -->
    <Group horizontalCenter="0" verticalCenter="-1">
        <Rect width="6" height="7">
            <fill>
                <SolidColor color="0xFFFFFF"  />
            </fill>
        </Rect>
        <Line yFrom="1" yTo="1" xFrom="0" xTo="6">
            <stroke>
                <SolidColorStroke color="0x3D5D92" pixelHinting="true" />
            </stroke>
        </Line>
        <Line yFrom="4" yTo="4" xFrom="0" xTo="6">
            <stroke>
                <SolidColorStroke color="0x3D5D92" pixelHinting="true" />
            </stroke>
        </Line>
        <Line yFrom="7" yTo="7" xFrom="0" xTo="6">
            <stroke>
                <SolidColorStroke color="0x3D5D92" pixelHinting="true" />
            </stroke>
        </Line>
    </Group>

</Skin>
