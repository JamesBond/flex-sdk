<Skin xmlns="http://ns.adobe.com/mxml/2009">

    <Metadata>
    	[HostComponent("flex.component.Button")]
    </Metadata> 
    
    <states>
        <State name="up" />
        <State name="over"/>
        <State name="down" />
        <State name="disabled" />
    </states>

    <content>

        <Group left="0" top="0" right="0" bottom="0">
            <!-- border -->
            <Rect left="0" top="0" right="0" bottom="0" minWidth="12" minHeight="12">
                <stroke>
                    <SolidColorStroke color="0x808080" color.disabled="0xC0C0C0" />
                </stroke>
            </Rect>
            <!-- fill -->
            <Rect left="1" top="1" right="1" bottom="1" minWidth="10" minHeight="10">
                <stroke>
                    <SolidColorStroke color="0xFFFFFF" color.over="0xFAFAFA" color.down="0xEFEFEF" />
                </stroke>
                <fill>
                    <SolidColor color="0xFFFFFF" color.over="0xF2F2F2" color.down="0xD8D8D8" />
                </fill>
            </Rect>
        </Group>

        <!-- Gripper -->
        <Group horizontalCenter="0" verticalCenter="-1">
            <Rect width="3" height="5">
                <fill>
                    <SolidColor color="0xFFFFFF" color.over="0xFAFAFA" color.down="0xEFEFEF" />
                </fill>
            </Rect>
            <Line xFrom="0" xTo="2" yFrom="1" yTo="1">
                <stroke>
                    <SolidColorStroke color="0x666666" color.over="0x616161" color.down="0x565656" pixelHinting="true" />
                </stroke>
            </Line>
            <Line xFrom="0" xTo="2" yFrom="3" yTo="3">
                <stroke>
                    <SolidColorStroke color="0x666666" color.over="0x616161" color.down="0x565656" pixelHinting="true" />
                </stroke>
            </Line>
            <Line xFrom="0" xTo="2" yFrom="5" yTo="5">
                <stroke>
                    <SolidColorStroke color="0x666666" color.over="0x616161" color.down="0x565656" pixelHinting="true" />
                </stroke>
            </Line>
        </Group>

    </content>

</Skin>
