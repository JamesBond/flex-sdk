<Skin xmlns="http://ns.adobe.com/mxml/2009">

    <Metadata>
    	[HostComponent("mx.components.FxButton")]
    </Metadata> 
    
    <states>
        <State name="up" />
        <State name="down" />
        <State name="over" />
        <State name="disabled" />
    </states>

    <!-- border -->
    <Rect left="0" right="0" top="0" bottom="0" radiusX="2" radiusY="2" width="5">
        <fill>
            <LinearGradient>
                <GradientEntry color="0x000000" alpha="0.55" />
                <GradientEntry color="0xFFFFFF" alpha="0.55" ratio="0.90" />
            </LinearGradient>
        </fill>
    </Rect>

    <!-- fill -->
    <Rect left="1" right="1" top="1" bottom="1" radiusX="2" radiusY="2">
        <fill>
            <SolidColor color="0xCACACA" />
        </fill>
    </Rect>
    
    <!-- shadow -->
    <Rect top="2" bottom="2" left="1" width="1">
        <fill>
            <SolidColor color="0x9E9E9E" />
        </fill>
    </Rect>
</Skin>
