<Skin xmlns="http://ns.adobe.com/mxml/2009">

    <Metadata>
    	[HostComponent("flex.component.Button")]
    </Metadata> 
    
    <states>
        <State name="up" />
        <State name="over"/>
        <State name="down" />
        <State name="disabled" />
    </states>

    <!-- background -->
    <Rect width="14" height="17">
        <stroke>
            <SolidColorStroke color="0x5380D0" color.disabled="0xA9C0E8" />
        </stroke>
        <fill>
            <SolidColor color="0xFFFFFF" color.over="0xEBF4FF" color.down="0xDEEBFF" />
        </fill>
    </Rect>
    <!-- arrow -->
    <Path>
        <fill>
            <SolidColor color="0x4E6290" color.disabled="0xA6B0C7" />
        </fill>
        <segments>
            <MoveSegment y="5" x="7.5" />
            <LineSegment y="12" x="11" />
            <LineSegment y="12" x="3.5" />
            <LineSegment y="5" x="7.5" />
        </segments>
    </Path>

</Skin>
