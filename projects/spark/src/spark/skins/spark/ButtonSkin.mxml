<Skin xmlns="http://ns.adobe.com/mxml/2009" minWidth="23" minHeight="23">

    <Metadata>
    	[HostComponent("mx.components.FxButton")]
    </Metadata>

    <states>
        <State name="up" />
        <State name="over" />
        <State name="down" />
        <State name="disabled" />
    </states>

    <!-- background -->
    <Rect left="0" right="0" top="0" bottom="0"
    	  width="70" height="23"
    	  radiusX="2" radiusY="2">
        <stroke>
            <SolidColorStroke color="0x5380D0" color.disabled="0xA9C0E8" />
        </stroke>
        <fill>
        	<SolidColor color="0xFFFFFF" color.over="0xEBF4FF" color.down="0xDEEBFF" />
        </fill>
    </Rect>

    <!-- label -->
	<TextBox id="labelField"
             fontFamily="Arial" fontSize="12"
             color="0x2B4381" color.disabled="0x95A1C0"
			 horizontalCenter="0" verticalCenter="1"
			 left="10" right="10" top="2" bottom="2"
			 textAlign="center" verticalAlign="middle">
	</TextBox>

</Skin>
