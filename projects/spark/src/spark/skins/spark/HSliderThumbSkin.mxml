<Skin xmlns="http://ns.adobe.com/mxml/2009">

    <Metadata>
    	[HostComponent("flex.component.Button")]
    </Metadata> 
    
    <states>
        <State name="up" />
        <State name="over" stateGroups="overAndDown" />
        <State name="down" stateGroups="overAndDown" />
        <State name="disabled" />
    </states>

    <Rect left="1" right="1" top="1" bottom="1" minWidth="15" minHeight="15" 
          radiusX="4" radiusY="4">
        <stroke>
            <SolidColorStroke color="0x3D5D92" color.disabled="0x9DADC9" />
        </stroke>
        <fill>
            <SolidColor color="0xFFFFFF" color.overAndDown="0xEEF3FF" />
        </fill>
    </Rect>

</Skin>
